{
  "require": {
    "craftcms/cms": "3.5.15.1",
    "nystudio107/craft-imageoptimize": "1.6.19",
    "nystudio107/craft-seomatic": "3.3.22",
    "vaersaagod/dospaces": "1.1.3",
    "verbb/field-manager": "^2.2",
    "verbb/formie": "1.2.26",
    "verbb/navigation": "^1.4",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
