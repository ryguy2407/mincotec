{
  "require": {
    "craftcms/cms": "3.6.6",
    "nystudio107/craft-imageoptimize": "1.6.24",
    "nystudio107/craft-seomatic": "3.3.32",
    "vaersaagod/dospaces": "1.1.3",
    "verbb/field-manager": "2.2.2",
    "verbb/formie": "1.3.18",
    "verbb/navigation": "1.4.13",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
